/*
 * cropBox.css
 * Stylesheet for crop.js
 */

.croppedHolder{
	position: absolute;
	margin-top: 30px;
	z-index: 999;
}
.croppedHolder_visible{
	position: absolute;
	margin-top: 30px;
	z-index: 500;
}
.cropPanel{
	position: absolute;
	height: auto;
	left: 0px;
	top: 0px;
	background-color: #BDBDBD;
}
.cropRegionClose{
	position: absolute;
	right: 0px;
	
}
.cropedHolder_inBckgd{
	position: absolute;
	margin-top: 30px;
	z-index: 1;
}
.crop_InBckGrnd{
	position: absolute;
	margin-top: 30px;
	z-index: -1;
}
