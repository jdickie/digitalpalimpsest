/* main panel styles --------------------------------------- */
div.panel {
    background:none;
	width:auto;
	height:auto;
	position:absolute;
	border:solid 3px #aadde1;
	z-index:900;
}div.panel_InBckGrnd {
    background:#d2b89c;
	width:800px;
	position:absolute;
	z-index:0;
	overflow:hidden;
}div.panel_annoOpen {
    background:#d2b89c;
	width:800px;	
	position:absolute;
	z-index:499;
	overflow:hidden;
}
/* main section styles --------------------------------------- */
.hd {  
	/* position: relative; */  
	text-align: left;
}.panelBody {
    /*overflow: hidden;
	height:auto;*/
	
}.ft {
	clear:both;
    position: relative;
    color: #291f1a;
}
/* hd 1st sub-section styles (quartoInfo div) --------------------------------------- */
.quartoInfo {
    /*float: bottom;*/
    display: block;
    margin-right: 0;
    z-index: 0;
	background:#985434;
	padding:5px;
	height:15px;
}.quartoInfoHeader {
    font-size: 11px;
    font-family: Verdana, Helvetica, Arial, sans-serif;
    position: relative;
    left: 0;
	float:left;
    color: #feecc7;
    text-decoration: none;
    text-transform: uppercase;
    /* z-index: -1; 
    filter: alpha( opacity = 65);
	opacity:.65;*/
}.quartoInfo .window_close {
	margin-top:0px;
	float:right;
}
/* hd 2nd sub-section styles (controls div) --------------------------------------- */ 
.controls {
	background:#987667;
	padding:5px;
}.pageControls, .zoomControls, .opacityControls, .imageToggle, .magControl {
    float: left;
    display: block;
    margin-right: 25px;
}.opacityControls {
    width: 100px;
}.imageToggle {
    padding-left: 5px;
}.pageBack, .pageNext, .zoomIn, .zoomOut, .darker, .lighter, .showImage, .showText, .magGlass, .dropDown {
    display: block;
 	float: left;
    margin-right: 1px;
    cursor: pointer;
}.zoomIn, .zoomOut, .darker, .lighter, .showImage {
	margin-right:7px;
}.pageBack {
    background: url("./images/icon_arrow_left.gif") no-repeat;
    width: 11px;
    height: 12px;
}.pageNext {
    background: url("./images/icon_arrow_right.gif") no-repeat;
    width: 11px;
    height: 12px;
}.zoomIn {
    width: 13px;
    height: 13px;
    background: url("./images/icon_plus.gif") no-repeat;
}.zoomOut {
    width: 13px;
    height: 13px;
    background: url("./images/icon_minus.gif") no-repeat;
}.darker {
    width: 13px;
    height: 13px;
    background: url("./images/icon_plus.gif") no-repeat right;
}.lighter {
    width: 13px;
    height: 13px;
    background: url("./images/icon_minus.gif") no-repeat right;
}.showImage {
    width: 27px;
    height: 12px;
    background: url("./images/icon_img.gif") no-repeat right;
}.showText {
    width: 29px;
    height: 12px;
    background: url("./images/icon_txt.gif") no-repeat right;
